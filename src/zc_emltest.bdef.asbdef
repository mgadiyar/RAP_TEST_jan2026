projection;
strict ( 2 );
use draft;
use side effects;

define behavior for ZC_EMLTEST alias Student
use etag
{
  use create;
  use update;
  use delete;

  use action Edit;
  use action Resume;
  use action Activate;
  use action Discard;
  use action Prepare;
  use action SetStud;

  use association _result { create; with draft; }
}

define behavior for ZC_STURESULTun alias Result
use etag
{
  use update;
  use delete;

  use association _Student { with draft; }
}